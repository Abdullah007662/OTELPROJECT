@model List<ResultLast4StaffDTO>
<div class="row">
    @foreach (var item in Model)
    {
        
    
    <div class="col-lg-3 col-sm-6">
        <div class="card">
            <div class="card-body">
                <div class="text-center">
                    <img src="@item.Image" class="rounded-circle" width="50" height="50" alt="">
                    <h5 class="mt-3 mb-1">@item.StaffName</h5>
                    <p class="m-0">@item.Description</p>
                </div>
            </div>
        </div>
    </div>
    }
</div>
